{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        // "semi": ["error", "always"]
        "linebreak-style": ["error", "windows"],
        "comma-dangle": ["error", {"functions": "never"}]
    }
}
